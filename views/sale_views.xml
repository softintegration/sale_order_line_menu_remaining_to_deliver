<?xml version="1.0" encoding="utf-8"?>
<odoo>

        <record id="view_order_line_tree_inherited" model="ir.ui.view">
			<field name="name">view.order.line.tree.inherited</field>
			<field name="model">sale.order.line</field>
			<field name="inherit_id" ref="sale.view_order_line_tree"/>
			<field name="arch" type="xml">
                <xpath expr="//field[@name='qty_delivered']" position="after">
                    <field name="remaining_to_deliver" optional="show"/>
				</xpath>
			</field>
		</record>


</odoo>
